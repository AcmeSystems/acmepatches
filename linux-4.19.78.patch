+			advref: fixed-regulator-advref {
+				compatible = "regulator-fixed";
+
+				regulator-name = "advref";
+				regulator-min-microvolt = <3300000>;
+				regulator-max-microvolt = <3300000>;
+				regulator-always-on;
+				regulator-boot-on;
+				vin-supply = <&vddana>;
+				status = "okay";
+			};
